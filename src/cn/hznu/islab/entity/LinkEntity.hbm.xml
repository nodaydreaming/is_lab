<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.hznu.islab.entity.LinkEntity" table="link" schema="is_lab">
        <id name="linkId" column="linkId">
            <generator class="native"/>
        </id>
        <property name="name" column="name" not-null="false"/>
        <property name="url" column="url" not-null="false"/>
        <property name="priority" column="priority" not-null="false"/>
    </class>
</hibernate-mapping>