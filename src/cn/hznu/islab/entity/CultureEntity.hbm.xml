<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.hznu.islab.entity.CultureEntity" table="culture" schema="is_lab">
        <id name="cultureId" column="cultureId">
            <generator class="native"/>
        </id>
        <property name="title" column="title" not-null="false"/>
        <property name="date" column="date" not-null="false"/>
        <property name="author" column="author" not-null="false"/>
        <property name="content" column="content" not-null="false"/>
        <property name="type" column="type" not-null="false"/>
    </class>
</hibernate-mapping>