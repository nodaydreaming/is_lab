<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.hznu.islab.entity.TeacherEntity" table="teacher" schema="is_lab">
        <id name="teacherId" column="teacherId">
            <generator class="native"/>
        </id>
        <property name="name" column="name" not-null="false"/>
        <property name="photo" column="photo" not-null="false"/>
        <property name="enName" column="en_name" not-null="false"/>
        <property name="gender" column="gender" not-null="false"/>
        <property name="telephone" column="telephone" not-null="false"/>
        <property name="email" column="email" not-null="false"/>
        <property name="researchArea" column="research_area" not-null="false"/>
        <property name="degree" column="degree" not-null="false"/>
        <property name="intro" column="intro" not-null="false"/>
        <property name="priority" column="priority" not-null="false"/>
    </class>
</hibernate-mapping>