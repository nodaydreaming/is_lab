<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.hznu.islab.entity.UserEntity" table="user" schema="is_lab">
        <id name="userId" column="userId">
            <generator class="native"/>
        </id>
        <property name="photo" column="photo" not-null="false"/>
        <property name="username" column="username" not-null="false"/>
        <property name="password" column="password" not-null="false"/>
        <property name="gender" column="gender" not-null="false"/>
        <property name="nickname" column="nickname" not-null="false"/>
        <property name="email" column="email" not-null="false"/>
        <property name="telephone" column="telephone" not-null="false"/>
        <property name="status" column="status" not-null="false"/>
    </class>
</hibernate-mapping>